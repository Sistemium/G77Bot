{
  "name": "g77bot",
  "version": "1.0.0",
  "description": "Telegram bot for G77",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "npm run lint",
    "babel-node": "babel-node",
    "start": "nodemon --exec 'npm run lint && babel-node' -- src/index.js",
    "pkg": "cp package.json dist",
    "build": "npm run lint && rm -rf dist && babel src -d dist && npm run pkg"
  },
  "dependencies": {
    "async": "^2.6.4",
    "aws-sdk": "^2.1200.0",
    "axios": "^0.27.2",
    "date-fns": "v2.0.0-alpha.8",
    "sistemium-debug": "^1.0.1",
    "lodash": "^4.17.21",
    "sistemium-telegram": "1.2.14",
    "sqs-consumer": "^3.8.0",
    "telegraf": "^3.40.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^1.19.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sistemium/G77Bot.git"
  },
  "author": "Sistemium UAB",
